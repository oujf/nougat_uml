"
      end

      operation 69713847 "setDisplay"
	public explicit_return_type "void"
	nparams 1
	  param inout name "sh" explicit_type "SurfaceHolder"
	cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${type} ${name}${(}${)}${const}${volatile}${throw}${abstract};
