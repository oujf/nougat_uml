	attribute 20426423 "COLOR_Format32bitABGR8888"
	  class_attribute const_attribute public explicit_type "int"
	  init_value " 0x7F00A000"
	  cpp_decl "    ${comment}${static}${thread_local}${mutable}${volatile}${const}${type} ${name}${value};
