  ${body}}
"
	
	
	
	comment "Don't throw IllegalStateException if there are too many images acquired.

@return Image if acquiring succeeded, or null otherwise.

@hide
"
      end

      operation 47953463 "acquireNextSurfaceImage"
	private explicit_return_type "int"
	nparams 1
	  param inout name "si" type class_ref 5894071 // SurfaceImage
	cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${type} ${name}${(}${)}${const}${volatile}${throw}${abstract};
