      attribute 20502967 "mListener"
	const_attribute private explicit_type "IMediaScannerListener.Stub"
	init_value " new IMediaScannerListener.Stub() {
        public void scanCompleted(String path, Uri uri) {
            MediaScannerConnectionClient client = mClient;
            if (client != null) {
                client.onScanCompleted(path, uri);
            }
        }
    }"
