      classrelation 12111031 // onCameraFrameAvailableListener (<unidirectional association>)
	relation 12111031 --->
	  a role_name "onCameraFrameAvailableListener" init_value "
            new SurfaceTexture.OnFrameAvailableListener() {
        @Override
        public void onFrameAvailable(SurfaceTexture surfaceTexture) {
            if (mLogVerbose) Log.v(TAG, \"New frame from camera\");
            synchronized(CameraSource.this) {
                mNewFrameAvailable = true;
                CameraSource.this.notify();
            }
        }
    }" private
	    cpp default "    ${comment}${static}${thread_local}${mutable}${volatile}${const}${type} * ${name}${value};
"
	    java "  ${comment}${@}${visibility}${static}${final}${transient}${volatile}${type} ${name}${value};
"
	    classrelation_ref 12111031 // onCameraFrameAvailableListener (<unidirectional association>)
	  b parent class_ref 6019511 // OnFrameAvailableListener
      end
