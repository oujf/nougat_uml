      classrelation 5144375 // mCurrentState (<directional composition>)
	relation 5144375 *-->
	  a role_name "mCurrentState" private
	    cpp default "    ${comment}${static}${thread_local}${mutable}${volatile}${const}${type} ${name}${value};
"
