	operation 40980151 "LatchedState"
	  cpp_inline public explicit_return_type ""
	  nparams 2
	    param inout name "parent" type class_ref 2555319 // Layer
	    param in name "initialValue" explicit_type "T"
	  cpp_decl "    ${comment}${inline}${name}${(}${t0} & ${p0}${v0}, ${t1} ${p1}${v1}${)}${volatile}${throw}${abstract} : mParent(parent),
                mPendingValue(initialValue),
                mValue(initialValue) {};
