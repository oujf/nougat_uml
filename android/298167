      operation 40979767 "setColorTransformHook"
	class_operation cpp_inline private explicit_return_type "int32_t"
	nparams 4
	  param inout name "device" explicit_type "hwc2_device_t"
	  param in name "display" explicit_type "hwc2_display_t"
	  param in name "" explicit_type "float"
	  param in name "intHint" explicit_type "int32_t"
	cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${type} ${name}${(}${t0} * ${p0}${v0}, ${t1} ${p1}${v1}, const ${t2} * ${p2}${v2}, ${t3} ${p3}${v3}${)}${const}${volatile}${throw}${abstract} {
        // We intentionally throw away the matrix, because if the hint is
        // anything other than IDENTITY, we have to fall back to client
        // composition anyway
