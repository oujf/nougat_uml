class FrameworkClient
!!!42689207.cpp!!!	FrameworkClient(in socket : int)
    mSocket = socket;
    pthread_mutex_init(&mWriteMutex, NULL);
!!!42689463.cpp!!!	sendMsg(in msg : char) : int
    int ret;
    if (mSocket < 0) {
        errno = EHOSTUNREACH;
        return -1;
    }

    pthread_mutex_lock(&mWriteMutex);
    ret = TEMP_FAILURE_RETRY(write(mSocket, msg, strlen(msg) +1));
    if (ret < 0) {
        SLOGW("Unable to send msg '%s' (%s)", msg, strerror(errno));
    }
    pthread_mutex_unlock(&mWriteMutex);
    return 0;
!!!42689591.cpp!!!	sendMsg(in msg : char, in data : char) : int
    size_t bufflen = strlen(msg) + strlen(data) + 1;
    char *buffer = (char *) alloca(bufflen);
    if (!buffer) {
        errno = -ENOMEM;
        return -1;
    }
    snprintf(buffer, bufflen, "%s%s", msg, data);
    return sendMsg(buffer);
