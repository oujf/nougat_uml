      operation 43075895 "SizedBuffer"
	cpp_inline public explicit_return_type ""
	nparams 1
	  param in name "length" type class_ref 2810935 // uint32_t
	cpp_decl "    ${comment}${inline}${name}${(}${t0} ${p0}${v0}${)}${volatile}${throw}${abstract} {
        if (length != 0) {
            buffer.reset(new uint8_t[length]);
        } else {
            buffer.reset();
        }
        this->length = length;
    };
"
