           int32_t rc = sync_wait(i->input_buffer->release_fence, TIMEOUT_NEVER);
           close(i->input_buffer->release_fence);
           if (rc != OK) {
               ALOGE("%s: input buffer sync wait failed %d", __func__, rc);
