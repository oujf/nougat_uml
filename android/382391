	operation 44076215 "setup_and_add"
	  cpp_inline public explicit_return_type "int"
	  nparams 3
	    param in name "id" explicit_type "int"
	    param inout name "session" type class_ref 3552951 // Session
	    param in name "appMsg" explicit_type "hub_message_t"
	  cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${type} ${name}${(}${t0} ${p0}${v0}, ${t1} * ${p1}${v1}, const ${t2} * ${p2}${v2}${)}${const}${volatile}${throw}${abstract} {
            Mutex::Autolock _l(lock);
            if (sessions_.count(id) == 0 && !session->isRunning()) {
                int ret = session->setup(appMsg);
                if (ret < 0) {
                    session->complete();
                } else {
                    sessions_[id] = session;
                }
                return ret;
            }
            return -EBUSY;
        };
"
	  java_def "  ${comment}${@}${visibility}${final}${static}${abstract}${synchronized}${type} ${name}${(}${)}${throws}${staticnl}{
  ${body}}
"
	  
	  
	  
	end
      end

