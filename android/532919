      operation 46867639 "createTyped"
	class_operation preserve_cpp_body_indent public explicit_return_type "sp<Allocation>"
	nparams 5
	  param in name "rs" explicit_type "sp<RS>"
	  param in name "type" explicit_type "sp<const Type>"
