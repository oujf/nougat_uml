      classrelation 17230263 // mPreparedListener (<unidirectional association>)
	relation 17230263 --->
	  a role_name "mPreparedListener" init_value " new MediaPlayer.OnPreparedListener() {
        public void onPrepared(MediaPlayer mp) {
            synchronized (prepareDone) {
                if(mPrepareReset){
                    Log.v(TAG, \"call Reset\");
                    mMediaPlayer.reset();
                }
                Log.v(TAG, \"notify the prepare callback\");
                prepareDone.notify();
                onPrepareSuccess = true;
            }
        }
    }" class_relation package
	    cpp default "    ${comment}${static}${thread_local}${mutable}${volatile}${const}${type} * ${name}${value};
"
	    java "  ${comment}${@}${visibility}${static}${final}${transient}${volatile}${type} ${name}${value};
"
	    classrelation_ref 17230263 // mPreparedListener (<unidirectional association>)
	  b parent class_ref 6094007 // OnPreparedListener
      end

