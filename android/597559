      classrelation 17682871 // FACTORY (<unidirectional association>)
	relation 17682871 --->
	  a role_name "FACTORY" init_value " new FilterFactory(\"not\") {
        @Override
        public Filter newFilter(XmlPullParser parser)
                throws IOException, XmlPullParserException {
            Filter child = null;
            int outerDepth = parser.getDepth();
            while (XmlUtils.nextElementWithin(parser, outerDepth)) {
                Filter filter = IntentFirewall.parseFilter(parser);
                if (child == null) {
                    child = filter;
                } else {
                    throw new XmlPullParserException(
                            \"<not> tag can only contain a single child filter.\", parser, null);
                }
            }
            return new NotFilter(child);
        }
    }" class_relation const_relation public
	    cpp default "    ${comment}${static}${thread_local}${mutable}${volatile}${const}${type} * ${name}${value};
"
	    java "  ${comment}${@}${visibility}${static}${final}${transient}${volatile}${type} ${name}${value};
"
	    classrelation_ref 17682871 // FACTORY (<unidirectional association>)
	  b parent class_ref 7715895 // FilterFactory
      end
