      classrelation 17685559 // FACTORY (<unidirectional association>)
	relation 17685559 --->
	  a role_name "FACTORY" init_value " new FilterFactory(\"sender-package\") {
        @Override
        public Filter newFilter(XmlPullParser parser)
                throws IOException, XmlPullParserException {
            String packageName = parser.getAttributeValue(null, ATTR_NAME);

            if (packageName == null) {
                throw new XmlPullParserException(
                    \"A package name must be specified.\", parser, null);
            }

            return new SenderPackageFilter(packageName);
        }
