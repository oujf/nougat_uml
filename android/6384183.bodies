class StatusBarState
!!!72037047.java!!!	toShortString(in x : int) : String
        switch (x) {
            case SHADE:
                return "SHD";
            case SHADE_LOCKED:
                return "SHD_LCK";
            case KEYGUARD:
                return "KGRD";
            case FULLSCREEN_USER_SWITCHER:
                return "FS_USRSW";
            default:
                return "bad_value_" + x;
        }
