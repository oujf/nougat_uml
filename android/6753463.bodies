class RenderingView
!!!75812919.java!!!	RenderingView(inout c : Context, in color : int)
            super(c);
            mColor = color;
!!!75813175.java!!!	onDraw(inout canvas : Canvas) : void
            super.onDraw(canvas);
            Paint p = new Paint();
            p.setColor(mColor);

            float yOffset = 0;
            for (int i = 0; i < 2; ++i) {
                float xOffset = 0;

                p.setStrokeWidth(0f);
                p.setStrokeCap(Paint.Cap.SQUARE);
                canvas.drawPoint(100 + xOffset, 100 + yOffset, p);
                xOffset += 5;

                p.setStrokeWidth(1f);
                canvas.drawPoint(100 + xOffset, 100 + yOffset, p);
                xOffset += 15;

                p.setStrokeWidth(20);
                canvas.drawPoint(100 + xOffset, 100 + yOffset, p);
                xOffset += 30;

                p.setStrokeCap(Paint.Cap.ROUND);
                canvas.drawPoint(100 + xOffset, 100 + yOffset, p);

                p.setAntiAlias(true);
                yOffset += 30;
            }

