!!!81138871.java!!!	replaceCharacterAndClose(inout replace : CharSequence) : void
        int selEnd = Selection.getSelectionEnd(mText);
        if (mInsert || selEnd == 0) {
            mText.insert(selEnd, replace);
        } else {
            mText.replace(selEnd - 1, selEnd, replace);
        }

        dismiss();
!!!81138999.java!!!	onClick(inout v : View) : void
        if (v == mCancelButton) {
