class CharacterPickerDialog
!!!81138487.java!!!	CharacterPickerDialog(inout context : Context, inout view : View, inout text : Editable, inout options : String, inout insert : boolean)
        super(context, com.android.internal.R.style.Theme_Panel);

        mView = view;
