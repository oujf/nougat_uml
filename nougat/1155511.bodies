class LoadCompleteCallback
!!!29188535.java!!!	onLoadComplete(inout soundPool : SoundPool, in sampleId : int, in status : int) : void
                synchronized(mSoundPool) {
                    if (DEBUG) Log.d(LOG_TAG, "Sample " + sampleId + " load status = " + status);
                    if (status != 0) {
                        mLoadStatus = status;
                    }
                    if (sampleId == mLastSample) {
                        mSoundPool.notify();
                    }
                }
