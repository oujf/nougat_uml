class ProximityEvaluator
!!!30516151.java!!!	evaluate(in value : float, in type : int) : float
        float evaluation = 0.0f;
        float threshold = 0.1f;
        if (type == Classifier.QUICK_SETTINGS) {
            threshold = 1.0f;
        }
        if (value >= threshold) evaluation += 2.0;
        return evaluation;
