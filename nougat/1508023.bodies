                    mService.mCompatModePackages.setPackageAskCompatModeLocked(
                            mAppInfo.packageName, mAlwaysShow.isChecked());
                    updateControls();
                }
            }
        });
        mHint = findViewById(com.android.internal.R.id.reask_hint);

