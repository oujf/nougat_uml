!!!32518839.java!!!	CompatModeDialog(inout service : ActivityManagerService, inout context : Context, inout appInfo : ApplicationInfo)
        super(context, com.android.internal.R.style.Theme_Holo_Dialog_MinWidth);
        setCancelable(true);
        setCanceledOnTouchOutside(true);
