	operation 2441271 "Builder"
	  public explicit_return_type ""
	  nparams 2
	    param in name "jobId" explicit_type "int"
	    param inout name "jobService" type class_ref 132279 // ComponentName
	  cpp_decl "    ${comment}${inline}${name}${(}${)}${volatile}${throw};
