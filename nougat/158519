      operation 3019959 "takePicture"
	public explicit_return_type "void"
	nparams 4
	  param inout name "shutter" type class_ref 456759 // ShutterCallback
	  param inout name "raw" type class_ref 456887 // PictureCallback
	  param inout name "postview" type class_ref 456887 // PictureCallback
	  param inout name "jpeg" type class_ref 456887 // PictureCallback
	cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${type} ${name}${(}${)}${const}${volatile}${throw}${abstract};
"
	cpp_def "${comment}${inline}${type} ${class}::${name}${(}${)}${const}${volatile}${throw}${staticnl}{
  ${body}}
