class ToggleAction
!!!33470903.java!!!	ToggleAction(in enabledIconResId : int, in disabledIconResid : int, in message : int, in enabledStatusMessageResId : int, in disabledStatusMessageResId : int)
            mEnabledIconResId = enabledIconResId;
            mDisabledIconResid = disabledIconResid;
            mMessageResId = message;
            mEnabledStatusMessageResId = enabledStatusMessageResId;
            mDisabledStatusMessageResId = disabledStatusMessageResId;
!!!33471031.java!!!	willCreate() : void

!!!33471159.java!!!	getLabelForAccessibility(inout context : Context) : CharSequence
            return context.getString(mMessageResId);
!!!33471287.java!!!	create(inout context : Context, inout convertView : View, inout parent : ViewGroup, inout inflater : LayoutInflater) : View
            willCreate();

            View v = inflater.inflate(R
                            .layout.global_actions_item, parent, false);

            ImageView icon = (ImageView) v.findViewById(R.id.icon);
            TextView messageView = (TextView) v.findViewById(R.id.message);
            TextView statusView = (TextView) v.findViewById(R.id.status);
            final boolean enabled = isEnabled();

            if (messageView != null) {
                messageView.setText(mMessageResId);
                messageView.setEnabled(enabled);
            }

            boolean on = ((mState == State.On) || (mState == State.TurningOn));
            if (icon != null) {
                icon.setImageDrawable(context.getDrawable(
                        (on ? mEnabledIconResId : mDisabledIconResid)));
                icon.setEnabled(enabled);
            }

            if (statusView != null) {
                statusView.setText(on ? mEnabledStatusMessageResId : mDisabledStatusMessageResId);
                statusView.setVisibility(View.VISIBLE);
                statusView.setEnabled(enabled);
            }
            v.setEnabled(enabled);

            return v;
!!!33471415.java!!!	onPress() : void
            if (mState.inTransition()) {
                Log.w(TAG, "shouldn't be able to toggle when in transition");
                return;
            }

            final boolean nowOn = !(mState == State.On);
            onToggle(nowOn);
            changeStateFromPress(nowOn);
!!!33471543.java!!!	isEnabled() : boolean
            return !mState.inTransition();
!!!33471671.java!!!	changeStateFromPress(inout buttonOn : boolean) : void
            mState = buttonOn ? State.On : State.Off;
!!!33471927.java!!!	updateState(inout state : GlobalActions::ToggleAction::State) : void
            mState = state;
