      operation 28016951 "PdfRenderer"
	public explicit_return_type ""
	nparams 1
	  param inout name "input" type class_ref 264887 // ParcelFileDescriptor
	nexceptions 1
	  explicit_exception "IOException"
	cpp_decl "    ${comment}${inline}${name}${(}${)}${volatile}${throw};
"
