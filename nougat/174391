      attribute 1879607 "RECOGNITION_FLAG_ALLOW_MULTIPLE_TRIGGERS"
	class_attribute const_attribute public explicit_type "int"
	init_value " 0x2"
	cpp_decl "    ${comment}${static}${mutable}${volatile}${const}${type} ${name}${value};
"
