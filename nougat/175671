      operation 4385591 "playEarcon"
	public explicit_return_type "int"
	nparams 4
	  param in name "earcon" type class_ref 129591 // String
	  param in name "queueMode" explicit_type "int"
	  param in name "params" type class_ref 131511 // Bundle
	  param in name "utteranceId" type class_ref 129591 // String
	cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${type} ${name}${(}${)}${const}${volatile}${throw}${abstract};
