	operation 28619191 "excludeRule"
	  public return_type class_ref 1076663 // Builder
	  nparams 2
	    param inout name "attrToMatch" type class_ref 385335 // AudioAttributes
	    param in name "rule" explicit_type "int"
	  nexceptions 1
	    explicit_exception "IllegalArgumentException"
	  cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${type} ${name}${(}${)}${const}${volatile}${throw}${abstract};
