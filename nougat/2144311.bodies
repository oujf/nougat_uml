!!!37615287.java!!!	getLocaleFromCarrierProperties(inout ctx : Context) : Locale
        String carrier = SystemProperties.get("ro.carrier");

        if (null == carrier || 0 == carrier.length() || "unknown".equals(carrier)) {
            return null;
