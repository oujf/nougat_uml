!!!38717111.java!!!	run() : void
        RenderScript pRS = RenderScript.create(mCtx);
        ScriptC_copy_test s = new ScriptC_copy_test(pRS);
        pRS.setMessageHandler(mRsMessage);

        testFloat2(pRS, s);
        testFloat3(pRS, s);
        testFloat4(pRS, s);
        s.invoke_sendResult(true);

        pRS.finish();
        waitForMessage();
        pRS.destroy();
