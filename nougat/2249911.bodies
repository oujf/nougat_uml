class UT_copy_test
!!!38716599.java!!!	UT_copy_test(inout rstc : RSTestCore, inout res : Resources, inout ctx : Context)
        super(rstc, "Copy", ctx);
        mRes = res;
!!!38716727.java!!!	testFloat2(inout rs : RenderScript, inout s : ScriptC_copy_test) : void
        Allocation a1 = Allocation.createSized(rs, Element.F32_2(rs), 1024);
        Allocation a2 = Allocation.createSized(rs, Element.F32_2(rs), 1024);

        float[] f1 = new float[1024 * 2];
        float[] f2 = new float[1024 * 2];
        for (int ct=0; ct < f1.length; ct++) {
            f1[ct] = (float)ct;
        }
        a1.copyFrom(f1);

        s.forEach_copyFloat2(a1, a2);

        a2.copyTo(f2);
        for (int ct=0; ct < f1.length; ct++) {
            if (f1[ct] != f2[ct]) {
                failTest();
                Log.v("RS Test", "Compare failed at " + ct + ", " + f1[ct] + ", " + f2[ct]);
            }
        }
        a1.destroy();
        a2.destroy();
