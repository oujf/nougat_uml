      operation 39045431 "completeQuery"
	public return_type class_ref 3005751 // status_t
	nparams 1
	  param in name "status" type class_ref 3005751 // status_t
	cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${type} ${name}${(}${t0} ${p0}${v0}${)}${const}${volatile}${throw}${abstract};
