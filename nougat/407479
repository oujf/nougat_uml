      end

      classrelation 10720183 // mOnScaleGestureListener (<unidirectional association>)
	relation 10720183 --->
	  a role_name "mOnScaleGestureListener" init_value "
            new ScaleGestureDetector.SimpleOnScaleGestureListener() {
        @Override
        public boolean onScale(ScaleGestureDetector detector) {
            mLiveScale *= detector.getScaleFactor();
            relayout();
            return true;
