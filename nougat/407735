	const_attribute private explicit_type "Runnable"
	init_value " new Runnable() {
        @Override
        public void run() {
            Slog.w(TAG, \"Stubborn dream did not finish itself in the time allotted\");
            stopDream(true /*immediate*/);
        }
    }"
