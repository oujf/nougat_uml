      operation 46000311 "blit"
	cpp_inline public explicit_return_type "bool"
	nparams 6
	  param in name "texName" type class_ref 4328759 // GLuint
	  param in name "texMatrix" explicit_type "float"
	  param in name "x" explicit_type "int32_t"
	  param in name "y" explicit_type "int32_t"
	  param in name "w" explicit_type "uint32_t"
	  param in name "h" explicit_type "uint32_t"
	cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${type} ${name}${(}${t0} ${p0}${v0}, const ${t1} * ${p1}${v1}, ${t2} ${p2}${v2}, ${t3} ${p3}${v3}, ${t4} ${p4}${v4}, ${t5} ${p5}${v5}${)}${const}${volatile}${throw}${abstract} {
        float modColor[4] = { 1.0f, 1.0f, 1.0f, 1.0f };
        return modBlit(texName, texMatrix, modColor, x, y, w, h);
    };
