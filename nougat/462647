      visibility package 
      cpp_decl "${comment}${template}class ${name}${inherit} {
${members}};
${inlines}
"
      java_decl "${comment}${@}${visibility}${final}${abstract}class ${name}${extends}${implements} {
${members}}
"
      php_decl ""
      python_2_2 python_decl ""
      idl_decl ""
      explicit_switch_type ""
      
      comment " ----------------------------------------------------------------------------
 An egl_display_ptr is a kind of smart pointer for egl_display_t objects.
 It doesn't refcount the egl_display_t, but does ensure that the underlying
 EGL implementation is \"awake\" (not hibernating) and ready for use as long
 as the egl_display_ptr exists."
      operation 45761079 "egl_display_ptr"
	cpp_inline public explicit_return_type ""
	nparams 1
	  param inout name "dpy" type class_ref 4442039 // egl_display_t
	cpp_decl "    ${comment}${inline}explicit ${name}${(}${t0} * ${p0}${v0}${)}${volatile}${throw} : mDpy(dpy) {
        if (mDpy) {
            if (CC_UNLIKELY(!mDpy->enter())) {
                mDpy = NULL;
            }
        }
    };
"
	java_def "  ${comment}${@}${visibility}${final}${synchronized}${name}${(}${)}${throws} {
  ${body}}
"
	
	
	
      end

      operation 45761207 "egl_display_ptr"
	cpp_inline public explicit_return_type ""
	nparams 1
	  param in name "other" type class_ref 4441911 // egl_display_ptr
	cpp_decl "    ${comment}${inline}${name}${(}const ${t0} & ${p0}${v0}${)}${volatile}${throw} : mDpy(other.mDpy) {
        if (mDpy) {
            mDpy->enter();
        }
    };
"
	java_def "  ${comment}${@}${visibility}${final}${synchronized}${name}${(}${)}${throws} {
  ${body}}
"
	
	
	
	comment " We only really need a C++11 move constructor, not a copy constructor.
 A move constructor would save an enter()/leave() pair on every EGL API
 call. But enabling -std=c++0x causes lots of errors elsewhere, so I
 can't use a move constructor until those are cleaned up.

 egl_display_ptr(egl_display_ptr&& other) {
     mDpy = other.mDpy;
     other.mDpy = NULL;
 }"
      end

