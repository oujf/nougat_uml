	cpp_inline public explicit_return_type "void"
	nparams 3
	  param in name "period" explicit_type "nsecs_t"
	  param in name "phase" explicit_type "nsecs_t"
	  param in name "referenceTime" explicit_type "nsecs_t"
	cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${type} ${name}${(}${t0} ${p0}${v0}, ${t1} ${p1}${v1}, ${t2} ${p2}${v2}${)}${const}${volatile}${throw}${abstract} {
        if (kTraceDetailedInfo) ATRACE_CALL();
        Mutex::Autolock lock(mMutex);
        mPeriod = period;
        mPhase = phase;
        mReferenceTime = referenceTime;
        ALOGV(\"[%s] updateModel: mPeriod = %\" PRId64 \", mPhase = %\" PRId64
                \" mReferenceTime = %\" PRId64, mName, ns2us(mPeriod),
                ns2us(mPhase), ns2us(mReferenceTime));
        mCond.signal();
    };
