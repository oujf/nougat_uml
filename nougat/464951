	  nparams 1
	    param in name "other" type class_ref 4557879 // Attribute
	  cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${type} ${name}${(}const ${t0} & ${p0}${v0}${)}${const}${volatile}${throw}${abstract} {
            // this places EGL_NONE at the end
            EGLint lhs(v);
