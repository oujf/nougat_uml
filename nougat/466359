	  comment " The fence is only valid when the buffer is dequeued, and should be
 -1 any other time. When valid, we own the fd, and must ensure it is
 closed: either by closing it explicitly when queueing the buffer,
 or by passing ownership e.g. to ANativeWindow::cancelBuffer()."
	end

	attribute 14387767 "dequeued"
	  public explicit_type "bool"
	  cpp_decl "    ${comment}${static}${mutable}${volatile}${const}${type} ${name}${value};
"
	  java_decl "  ${comment}${@}${visibility}${static}${final}${transient}${volatile}${type} ${name}${value};
"
	  php_decl ""
	  python_decl ""
	  idl_decl ""
	end
      end
    end
  end

