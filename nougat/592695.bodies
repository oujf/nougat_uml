class MediaStore
!!!4210871.java!!!	getMediaScannerUri() : Uri
        return Uri.parse(CONTENT_AUTHORITY_SLASH + "none/media_scanner");
!!!4210999.java!!!	getVersion(inout context : Context) : String
        Cursor c = context.getContentResolver().query(
                Uri.parse(CONTENT_AUTHORITY_SLASH + "none/version"),
                null, null, null, null);
        if (c != null) {
            try {
                if (c.moveToFirst()) {
                    return c.getString(0);
                }
            } finally {
                c.close();
            }
        }
        return null;
