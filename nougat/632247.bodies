class Reverser
!!!4442679.java!!!	Reverser(inout source : CharSequence, in start : int, in end : int)
            mSource = source;
            mStart = start;
            mEnd = end;
!!!4442807.java!!!	length() : int
            return mEnd - mStart;
!!!4442935.java!!!	subSequence(in start : int, in end : int) : CharSequence
            char[] buf = new char[end - start];

            getChars(start, end, buf, 0);
            return new String(buf);
!!!4443063.java!!!	toString() : String
            return subSequence(0, length()).toString();
!!!4443191.java!!!	charAt(in off : int) : char
            return AndroidCharacter.getMirror(mSource.charAt(mEnd - 1 - off));
!!!4443319.java!!!	getChars(in start : int, in end : int, inout dest : char, in destoff : int) : void
            TextUtils.getChars(mSource, start + mStart, end + mStart,
                               dest, destoff);
            AndroidCharacter.mirror(dest, 0, end - start);

            int len = end - start;
            int n = (end - start) / 2;
            for (int i = 0; i < n; i++) {
                char tmp = dest[destoff + i];

                dest[destoff + i] = dest[destoff + len - i - 1];
                dest[destoff + len - i - 1] = tmp;
            }
