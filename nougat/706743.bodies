!!!5329719.java!!!	handleClickEvent(inout context : Context) : void
            // Show a simple popup dialog containing the description
            // string of the plugin.
            if (mDialog == null) {
                mDialog = new AlertDialog.Builder(context)
                        .setTitle(mName)
                        .setMessage(mDescription)
                        .setPositiveButton(R.string.ok, this)
                        .setCancelable(false)
                        .show();
            }
!!!5329847.java!!!	onClick(inout dialog : DialogInterface, in which : int) : void
            mDialog.dismiss();
            mDialog = null;
